<h1>Show User</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Permissions:</strong>
    <% for {k, v} <- @user.permissions do %>
      <%= k %>
      <%= v %>
    <% end %>
  </li>

  <li>
    <strong>Auth0:</strong>
    <%= @user.auth0_id %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
